{
    "root": true,
    "env": {
        "node": true,
        "browser": true,
        "es2021": true,
        "vue/setup-compiler-macros":true
    },
    "extends": [
        "plugin:vue/vue3-recommended",
        "plugin:@typescript-eslint/recommended",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/no-explicit-any": ["off"]
    }
}
